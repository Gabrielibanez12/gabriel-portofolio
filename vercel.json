{
    "version": 2,
    "builds": [
      {
        "src": "index.html",
        "use": "@vercel/static"
      },
      {
        "src": "styles/(.*)",
        "use": "@vercel/static"
      },
      {
        "src": "js/(.*)",
        "use": "@vercel/static"
      },
      {
        "src": "img/(.*)",
        "use": "@vercel/static"
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "/index.html"
      }
    ]
  }
  